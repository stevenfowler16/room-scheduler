{
  "name": "netlify-fauna",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@analytics/google-analytics": "^0.3.1",
    "@octokit/core": "^3.3.0",
    "analytics": "^0.3.5",
    "faunadb": "^2.13.1",
    "inquirer": "^8.0.0",
    "markdown-magic": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "bootstrap": "netlify dev:exec node ./scripts/bootstrap-fauna-database.js",
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "start": "netlify dev",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build": "react-scripts build"
  },
  "devDependencies": {
    "netlify-cli": "^3.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
